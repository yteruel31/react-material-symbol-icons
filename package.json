  "description": "Package for using material symbols from Google on React",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/lib/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:prepare && esno scripts/build",
    "build-storybook": "storybook build",
    "build:prepare": "npm run clean",
    "check": "npm run typecheck && npm run lint && npm run prettier:test",
    "clean": "rimraf dist",
    "generate:keys": "esno scripts/generate-icon-keys",
    "lint": "eslint src src --ext .ts,.tsx --cache",
    "prettier:test": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "storybook": "storybook dev -p 6006",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yteruel31/react-material-symbols.git"
  },
