    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addons": "^7.6.7",
    "@storybook/api": "^7.6.7",
    "@storybook/components": "^7.6.7",
    "@storybook/core-events": "^7.6.7",
    "@storybook/react": "^7.6.7",
    "@storybook/react-webpack5": "^7.6.7",
    "@types/fs-extra": "^11.0.1",
